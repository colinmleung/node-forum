doctype html
html(lang="en")
head
  title Log In
body
  h1 Log In
  form(name="logIn", method="POST")
    label(for="username") Username:
    input(id="username", name="username", type="text")
    br
    label(for="password") Password:
    input(id="password", name="password", type="password")
    br
    input(type="submit", value="Submit")